{
  "name": "@ember/addon-blueprint",
  "version": "0.17.0",
  "description": "Blueprint for scaffolding ember v2 addons",
  "keywords": [
    "ember-blueprint"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:ember-cli/ember-addon-blueprint.git"
  },
  "license": "MIT",
  "author": "",
  "scripts": {
    "lint": "prettier --check .",
    "format": "prettier --write .",
    "new:addon": "rm -rf my-addon; pnpm dlx ember-cli@latest addon my-addon --blueprint ."
  },
  "devDependencies": {
    "prettier": "^3.5.3",
    "prettier-plugin-ember-template-tag": "2.1.0",
    "release-plan": "^0.16.0"
  },
  "dependencies": {
    "sort-package-json": "^3.4.0"
  },
  "packageManager": "pnpm@10.6.5"
}
